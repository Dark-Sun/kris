<div id="content">
<div class="log_in">
<h1> Вхід </h1>

<%= form_tag sessions_path do %>
<p>
	<%= label_tag "Логін" %> <br>
	<%= text_field_tag :name, params[:name] %>
</p>
<p>
	<%= label_tag "Пароль" %> <br>
	<%= password_field_tag :password %>
</p>
<p class="button"> <%= submit_tag "Вхід" %></p>

<% end %>
</div>
</div>
